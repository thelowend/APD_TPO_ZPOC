package edu.uade.apdzpoc.excepciones;

public class ComunicationException extends Exception {

	public ComunicationException() {
		// TODO Auto-generated constructor stub
	}

	public ComunicationException(String arg0) {
		super(arg0);
		// TODO Auto-generated constructor stub
	}

	public ComunicationException(Throwable arg0) {
		super(arg0);
		// TODO Auto-generated constructor stub
	}

	public ComunicationException(String arg0, Throwable arg1) {
		super(arg0, arg1);
		// TODO Auto-generated constructor stub
	}

	public ComunicationException(String arg0, Throwable arg1, boolean arg2, boolean arg3) {
		super(arg0, arg1, arg2, arg3);
		// TODO Auto-generated constructor stub
	}

}
